students = [1,1,1,0,0,1]
sandwiches = [1,0,0,0,1,1]

from collections import deque

q = deque(students)
i = 0
count = 0

while q and count < len(q):
    if q[0] == sandwiches[i]:
        q.popleft()
        i += 1
        count = 0
    else:
        q.append(q.popleft())
        count += 1

print(len(q))
